UsersBundle\Entity\Pedido:
    type: entity
    table: pedido
    manyToOne:
        usuario:
            targetEntity: Usuario
            inversedBy: pedidos
            joinColumn:
                name: usuario_id
                referencedColumnName: id
            cascade: ["persist"]
    repositoryClass: UsersBundle\Repository\PedidoRepository
    id:
        id:
            type: integer
            id: true
            generator:
                strategy: AUTO
    fields:
        fecha:
            type: datetime
        importe:
            type: float
    lifecycleCallbacks: {  }
